@model SaralESuvidha.ViewModel.RetailUserViewModel

@{
    Layout = "_LayoutSysAdmin";
    ViewData["title"] = "Create Master Distributor - Result";
    if (Context.Session.TryGetValue("Controller", out byte[] cont))
    {
        var st = System.Text.Encoding.UTF8.GetString(cont);
        ViewData["Controller"] = st;
    }
}


    <div class="callout callout-@(Model.OperationMessage.Contains("Error:") ? "danger" : "success")">
        <h5>Master Distributor Name - @Model.FirstName @Model.MiddleName @Model.LastName</h5>
        <p>City - @Model.City</p>
        <p>Mobile Number - @Model.Mobile</p>
        <hr>
        <p class="text-@(Model.OperationMessage.Contains("Error:") ? "danger" : "success")"><strong>@Model.OperationMessage</strong></p>
        <p class="text-danger"><strong>Account is not yet active. Please complete KYC for activation.</strong></p>
    </div>

    <div class="card-body" style="padding-left: 0px;">
    <a class="btn btn-primary" asp-controller='@ViewData["controller"].ToString()' asp-action="Index">Dashboard</a>
    <a class="btn btn-primary" asp-controller='@ViewData["controller"].ToString()' asp-action="CreateMasterDistributor">Create New Master Distributor</a>
    </div>
