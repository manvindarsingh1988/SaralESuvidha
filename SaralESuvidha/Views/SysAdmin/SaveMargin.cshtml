@model SaralESuvidha.Models.UtilityMargin

@{
    Layout = "_LayoutSysAdmin";
    ViewData["title"] = "Save Margin For Master Distributor - Result";
    if (Context.Session.TryGetValue("Controller", out byte[] cont))
    {
        var st = System.Text.Encoding.UTF8.GetString(cont);
        ViewData["Controller"] = st;
    }
}


<div class="callout callout-@(Model.OperationMessage.Contains("Error:") ? "danger" : "success")">
    <h5>Master Distributor Name - @Model.FullName</h5>
    <p>OperatorName - @Model.OperatorName</p>
    <p>Margin Percent UPTO 200 Full payment - @Model.MarginPercentUpto200 %</p>
    <p>Margin Amount (201-4000 Full Payment) - @Model.FixMargin</p>
    <p>Margin Percent - @Model.MarginPercent %</p>
    <p>Max Margin - @Model.MaxMargin</p>
    <hr>
    <p class="text-@(Model.OperationMessage.Contains("Error:") ? "danger" : "success")"><strong>@Model.OperationMessage</strong></p>
</div>

<div class="card-body" style="padding-left: 0px;">
    <a class="btn btn-primary" asp-controller='@ViewData["controller"].ToString()' asp-action="Index">Dashboard</a>
    <a class="btn btn-primary" asp-controller='@ViewData["controller"].ToString()' asp-action="SetMargin">Set Margin For Another Master Distributor</a>
</div>