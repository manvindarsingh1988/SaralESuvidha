<form id="frmReport" method="post" action="#">
    <div class="card">
        <h6 class="card-header alert-primary" id="lblReport">
            Report
        </h6>
        <div class="card-datatable table-responsive">
            <table class="table table-striped table-bordered" id="datatable-adminreport" data-url="">
                <thead>
                    <tr>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</form>
<script type="text/javascript">
    $(document).ready(function () {
        LoadReport();
    });

    function LoadReport() {
        if ($.fn.DataTable.isDataTable('#datatable-adminreport')) {
            $('#datatable-adminreport').DataTable().destroy();
        }
        $('#datatable-adminreport tbody').empty();
        $('#datatable-adminreport thead').empty();

        var $table = $('#datatable-adminreport');
        $table.dataTable({
            iDisplayLength: 50,
            bAutoWidth: true,
            sAjaxSource: '@Url.Action("MarginSheetList", "CommonAuthAction")',
            cache: false,
            order: [[1, "asc"], [2, "asc"]],
            columns: [
                { data: "ID", title: "Id", visible: false } , { data: "RetailUserid", title: "User Detail" },
                { data: "OperatorName", title: "Operator Name" }, { data: "OperatorCircle", title: "Operator Circle", visible: false }, { data: "FixMargin", title: "Margin Upto 2000" }, { data: "MarginPercent", title: "Margin Percent" },
                { data: "MaxMargin", title: "Max Margin" }, { data: "Active", title: "Active" }
            ]
        });

        $('#lblReport').html('Margin Sheet List');
    }


</script>